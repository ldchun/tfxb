@import './asset/css/weui.wxss';
/* reset */
view,image,text,input,navigator,image,textarea,checkbox,radio,button,canvas,form,page{margin:0;padding:0;box-sizing:border-box;}
image,textarea{max-width:100%;}
textarea{z-index:0;}
checkbox,radio{margin-right:10rpx;}
button{margin-top:10rpx;margin-bottom:10rpx;}
form{width:100%;}
page{position:relative;min-height:100%;font-size:16px;color:#FFF;background:#445DB1 no-repeat top center;background-size:100% auto;background-attachment:fixed;}
page{background-image:url("data:image/jpg;base64,");}
/* common */
.clearfix:after, .clearft:after{clear:both;content:'';display:block;height:0;line-height:0;visibility:hidden;overflow:hidden;}
.clearfix, .clearft{zoom:1;}
.fl{float:left;}
.fr{float:right;}
.clear{clear:both;}
.allwh{width:100%;height:100%;}
.nopadd{padding:0;}
.nopadtb{padding-top:0;padding-bottom:0;}
.bghd{background-color:#F8F8F8;}
.text-center{text-align:center;}
.slbg{background:#e4007f;}
.slcolor{color:#e4007f;}
.flex-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;}
.flex-item{-webkit-box-flex:1;-webkit-flex:1;flex:1;}
.slcard{margin:10px 0;padding:10px 12px;background:#FFF;}
/*sl*/
.slhd,.slft{position:relative;min-height:46px;line-height:46px;background:#FFF;}
.slhd{border-bottom:1px solid #EEE;}
.slft{border-top:1px solid #EEE;}
.slbd{position:relative;padding:1px 0;}
.slhd.slfix, .slft.slfix{position:fixed;z-index:900;left:0;width:100%;}
.slhd.slfix{top:0;}
.slft.slfix{bottom:0;}
.slhd.nobd, .slft.nobd{border:0 none;}
.slhd.slfix + .slbd,.slhd.slfix ~ .slbd{padding-top:46px;}
.slft.slfix + .slbd,.slft.slfix ~ .slbd{padding-bottom:46px;}
.slhd-back, .slhd-tool{position:absolute;top:0;width:50px;height:100%;padding:0 10px;color:#333;text-align:center;line-height:45px;}
.slhd-back{left:0;}
.slhd-tool{right:0;}
.slhd-back i,.slhd-tool i{font-size:24px;color:#333;line-height:1;}
.slhd-back:hover,.slhd-tool:hover{background:#F9F9F9;border-color:#F9F9F9;}
.slhd-back:focus,.slhd-back:active,.slhd-tool:focus,.slhd-tool:active{background:#E2E2E2;}
.slhd-con{margin:0 50px;font-size:17px;line-height:46px;color:#333;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.slhd.nohd{background:transparent;border:0 none;}
.slhd.nohd .slhd-con{opacity:0;-webkit-transition:all 300ms ease-in-out;transition:all 300ms ease-in-out;}
.slhide{display:none;}
.slpadd-in{padding:15px;}
.slpadd-lf{padding:0 15px;}
.slpadd-tb{padding:15px 0;}
.slmarg-in{margin:10px;}
.slmarg-lf{margin:0 10px;}
.slmarg-tb{margin:10px 0;}
.slmarg-b{margin-bottom:10px;}
.sl-textarea{width:100%;height:auto;word-break:break-all;}
/*pop*/
.sloverlay{display:none;position:fixed;z-index:900;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0.6);}
.slpop{display:none;position:absolute;z-index:1000;top:50%;left:50%;width:260px;min-height:140px;margin:-70px 0 0 -130px;background:#FFF;
border-radius:5px;overflow:hidden;}
.slpop-hd{height:46px;text-align:center;font-size:16px;font-weight:bold;line-height:46px;}
.slpop-bd{position:relative;padding:10px;margin-bottom:10px;}
textarea.pop-textarea{display:block;width:100%;height:20px;min-height:20px;padding:0 5px;word-break:break-all;}
.slpop-inline{height:8px;margin-top:-5px;border:1px solid #D2D2D2;border-top:0 none;}
.slpop-op{text-align:center;border-top:1px solid #E2E2E2;}
.slpop-op button{margin:0;font-size:16px;border:0 none;border-radius:0;background:#FFF;}
.slpop-op button::after{display: none;}
.slpop-op .op-save{color:#349E7C;border-left:1px solid #E2E2E2;}
/*slcell*/
.slcell{position:relative;display:-webkit-box;display:-webkit-flex;display:flex;margin:5px 0;padding:10px 0;border-top:1px dashed #EEE;}
.slcell:first-child{border:0 none;}
.slcell.slcell-must:before{content:"*";position:absolute;left:-10px;top:13px;font-size:16px;color:#F00;}
.slcell-hd{width:90px;font-size:14px;color:#666;}
.slcell-bd{-webkit-box-flex:1;-webkit-flex:1;flex:1;}
.slcell-textarea{width:100%;height:auto;word-break:break-all;}
.slcell-bd input,.slcell-bd textarea{font-size:14px;color:#333;}
.picker{height:26px;}
.slcell-ft{padding-left:5px;text-align:right;color:#AAA;}
/*btn*/
button.hidebtn{background:transparent;border:0 none;border-radius:0;}
button.hidebtn::before,button.hidebtn::after{display:none;}
.slbtn{margin:20px 0;}
.slbtn button{width:200px;margin:15px auto;font-size:15px;border-radius:35px;}
.slbtn button[type="primary"]{background-color:#349E7C;}
.slbtn button[type="default"]{color:#349E7C;background-color:#FFF;border:1px solid #349E7C;}
.slbtn button[type="primary"].btn-hover{background-color:#308A6D;}
.slbtn button[type="default"].btn-hover{background-color:#E2E2E2;}
/* opbtn */
button.btnclick-anim.button-hover{-webkit-transform:translateY(5px);transform:translateY(5px);}
.opbtn:before,.opbtn:after{display:none;}
button.opbtn{display:inline-block;max-width:100%;height:40px;margin:0;padding:0 15px;line-height:38px;font-size:16px;border:1px solid #D2D2D2;border-radius:1px;}
button.opbtn.btn-block{display:block;}
button.opbtn.btn-inner{color:#FFF;background:#e4007f;border:1px solid #e4007f;}
button.opbtn.btn-inner.btn-hover{background:#dd3d97;}
/*开启硬件加速*/
.speedup{-webkit-transform:translateZ(0);-moz-transform:translateZ(0);-ms-transform:translateZ(0);-o-transform:translateZ(0);transform:translateZ(0);}
@-webkit-keyframes fadeIn{
  0%{opacity:0;}
  100%{opacity:1;}
}
@keyframes fadeIn{
  0%{opacity:0;}
  100%{opacity:1;}
}
@-webkit-keyframes fadeInUp{
  0%{opacity:0;-webkit-transform: translate3d(0, 50px, 0);transform: translate3d(0, 50px, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@keyframes fadeInUp{
  0%{opacity:0;-webkit-transform: translate3d(0, 50px, 0);transform: translate3d(0, 50px, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@-webkit-keyframes fadeInLeft{
  0%{opacity:0;-webkit-transform: translate3d(-50px, 0, 0);transform: translate3d(-50px, 0, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@keyframes fadeInLeft{
  0%{opacity:0;-webkit-transform: translate3d(-50px, 0, 0);transform: translate3d(-50px, 0, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@-webkit-keyframes fadeInRight{
  0%{opacity:0;-webkit-transform: translate3d(50px, 0, 0);transform: translate3d(50px, 0, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@keyframes fadeInRight{
  0%{opacity:0;-webkit-transform: translate3d(50px, 0, 0);transform: translate3d(50px, 0, 0);}
  100%{opacity:1;-webkit-transform: none;transform: none;}
}
@-webkit-keyframes slideInDown {
  0% {-webkit-transform: translate3d(0, -100%, 0);transform: translate3d(0, -100%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@keyframes slideInDown {
  0% {-webkit-transform: translate3d(0, -100%, 0);transform: translate3d(0, -100%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@-webkit-keyframes slideInUp {
  0% {-webkit-transform: translate3d(0, 100%, 0);transform: translate3d(0, 100%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@keyframes slideInUp {
  0% {-webkit-transform: translate3d(0, 100%, 0);transform: translate3d(0, 100%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@-webkit-keyframes slideInUpBig {
  0% {-webkit-transform: translate3d(0, 300%, 0);transform: translate3d(0, 300%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@keyframes slideInUpBig {
  0% {-webkit-transform: translate3d(0, 300%, 0);transform: translate3d(0, 300%, 0);visibility: visible;}
  100% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
}
@-webkit-keyframes slideOutUp {
  0% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
  100% {-webkit-transform: translate3d(0, -100%, 0);transform: translate3d(0, -100%, 0);visibility: hidden;}
}
@keyframes slideOutUp {
  0% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
  100% {-webkit-transform: translate3d(0, -100%, 0);transform: translate3d(0, -100%, 0);visibility: hidden;}
}
@-webkit-keyframes slideOutDown {
  0% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
  100% {-webkit-transform: translate3d(0, -100%, 0);transform: translate3d(0, -100%, 0);visibility: hidden;}
}
@keyframes slideOutDown {
  0% {-webkit-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);}
  100% {-webkit-transform: translate3d(0, 100%, 0);transform: translate3d(0, 100%, 0);visibility: hidden;}
}
@-webkit-keyframes zoomIn {
  0% {opacity: 0;-webkit-transform: scale3d(0.3, 0.3, 0.3);transform: scale3d(0.3, 0.3, 0.3);}
  50% {opacity: 1;}
}
@keyframes zoomIn {
  0% {opacity: 0;-webkit-transform: scale3d(0.3, 0.3, 0.3);transform: scale3d(0.3, 0.3, 0.3);}
  50% {opacity: 1;}
}
@-webkit-keyframes pulse {
  0% { -webkit-transform: scale3d(1, 1, 1); transform: scale3d(1, 1, 1); }
  50% { -webkit-transform: scale3d(1.05, 1.05, 1.05); transform: scale3d(1.05, 1.05, 1.05); }
  100% { -webkit-transform: scale3d(1, 1, 1); transform: scale3d(1, 1, 1); }
}
@keyframes pulse {
  0% { -webkit-transform: scale3d(1, 1, 1); transform: scale3d(1, 1, 1); }
  50% { -webkit-transform: scale3d(1.05, 1.05, 1.05); transform: scale3d(1.05, 1.05, 1.05); }
  100% { -webkit-transform: scale3d(1, 1, 1); transform: scale3d(1, 1, 1); }
}
@-webkit-keyframes jump {
  0% { -webkit-transform: translate(0px, 0px); transform: translate(0px, 0px);}
  50% { -webkit-transform: translate(0px, -10px); transform: translate(0px, -10px);}
  100% { -webkit-transform: translate(0px, 0px); transform: translate(0px, 0px); }
}
 @keyframes jump {
  0% { -webkit-transform: translate(0px, 0px); transform: translate(0px, 0px);}
  50% { -webkit-transform: translate(0px, -10px); transform: translate(0px, -10px);}
  100% { -webkit-transform: translate(0px, 0px); transform: translate(0px, 0px); }
}
.animated{animation-duration:1s;animation-delay:0;animation-timing-function:ease-in-out;animation-fill-mode:both;}
.anim-star{-webkit-transition:width 1000ms linear;transition:width 1000ms linear;}
/* button */
button[type="primary"]{background:#02B265;}
button.msgbtn{display:inline-block;margin:0;padding:0;line-height:inherit;color:inherit;border:0 none;border-radius:0;background:transparent;}
button.msgbtn::before,button.msgbtn::after{display:none;}
button.formbtn{display:block;margin:0;padding:0;font-size:inherit;color:inherit;line-height:inherit;text-align:inherit;border:0 none;
    border-radius:0;background:inherit;}
button.formbtn::before, button.formbtn::after{display:none;}
/*垂直居中*/
.vhcenter{font-size:0;text-align:center;}
.vhcenter:after{content:'';display:inline-block;width:0;height:100%;opacity:0;vertical-align:middle;}
.vhcenter>.vhcon{display:inline-block;font-size:14px;line-height:1.2;vertical-align:middle;}
/*note */
.note ~ .note{margin-top:10px;}
.note .note-hd + .note-bd{margin-top:8px;}
.note .note-hd{font-size:16px;color:#333;}
.number-box .number-reduce,.number-box .number-add,.number-box .number-in{float:left;height:28px;margin:0;text-align:center;line-height:26px;border:1px solid #C0C0C0;border-radius:0;}
.number-box .number-reduce,.number-box .number-add{padding:0 10px;}
.number-box button{font-size:18px;border-radius:0!important;}
.number-box button::before,.number-box button::after{display:none;}
.number-box button.button-hover{color:#332C2B;background:#D2D2D2;}
.number-box .number-in{width:50px;border-left:0 none;border-right:0 none;}
.weui-cells{font-size:15px;}
.weui-cells:before, .weui-cells:after{border:0 none;}
.weui-cell_switch{padding-top:8px;padding-bottom:8px;}
/*userface*/
.user-box{margin:15px 0;text-align:center;}
.face-box{width:80px;height:80px;margin:0 auto;padding:4px;border:1px solid #FFF;border-radius:50%;}
.user-box .user-face{display:block;width:70px;height:70px;margin:0 auto;border-radius:50%;}
.user-box .user-nick{color:#FFF;font-size:16px;line-height:1.2;}
/*level-bar*/
.level-box{position:relative;max-width:350px;margin:20px auto;padding:0 60px;color:#FFF;}
.level-con,.level-tip{position:relative;width:100%;}
.level-bar{position:relative;width:100%;height:12px;background:#FFF;border-radius:30px;overflow:hidden;}
.level-bar .bar-hl{position:absolute;left:0;top:0;width:50%;height:100%;background:#F8DE00;}
.level-con .level-min,.level-con .level-max{display:block;position:absolute;top:0;height:12px;line-height:12px;font-size:18px;white-space:nowrap;}
.level-con .level-min{right:100%;padding-right:10px;}
.level-con .level-max{left:100%;padding-left:10px;}
.level-tip .tip-val{position:absolute;left:50%;top:8px;margin-left:-12px;font-size:12px;line-height:1;white-space:nowrap;}
/*star */
.iconstar-bg, .iconstar{display:inline-block;height:18px;background:repeat-x;background-size:20px auto;}
.iconstar-bg, .iconstar{background-image:url("data:image/png;base64,");}
.iconstar-bg{position:relative;width:60px;background-position:0 -20px;}
.iconstar-bg .iconstar{position:absolute;left:0;top:0;}
.iconstar.star0{width:0;}
.iconstar.star1{width:20px;}
.iconstar.star2{width:40px;}
.iconstar.star3{width:60px;}
/* info-list */
.info-wrap{text-align:center;}
.info-list{display:inline-block;padding:10px 0;}
.info-list .info-li{height:28px;line-height:28px;margin:15px 0;font-size:26px;text-align:left;}
.info-list .info-li .info-title,.info-list .info-li .info-con{display:inline-block;height:28px;vertical-align:middle;}
.info-list .info-li image{height:30px;}
.info-list .info-li .info-con{margin-left:10px;font-weight:bold;letter-spacing:1.5px;}
.info-li image.img-this{width:150px;}
.info-li image.img-all{width:111px;}
.info-li image.img-sort{width:143px;}
.info-li image.img-level{width:142px;}
/*handle-btn  */
.handle-list{padding:5px 0;}
.handle-list .handle-row{margin-top:20px;text-align:center;}
.handle-list .handle-row:first-child{margin-top:0;}
.handle-row .handle-btn{width:260px;max-width:60%;height:auto;margin:0 auto;line-height:1.6;color:#FFF;background:transparent;border:0 none;}
.handle-row button.handle-btn::before,.handle-row button.handle-btn::after{display:none;}
.handle-row button.handle-btn.haschoice.button-hover{border:0 none;-webkit-transform:translateY(5px);transform:translateY(5px);}
.handle-row button.handle-btn.invalid.button-hover{border:0 none;-webkit-transform:translateY(0);transform:translateY(0);}
.handle-btn image{display:block;width:100%;height:auto;}
.handle-btn .handle-more{font-size:15px;letter-spacing:1px;}
