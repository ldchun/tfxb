<view class='slbd sign '>
    <view class='sign-title animated anim-title'>
        <image class="img-title" src="../../asset/image/sign-title.png" mode="widthFix"></image>
    </view>
    <view class='sign-map'>
        <image class="img-map map-bg animated anim-map" src="../../asset/image/sign-map.png" mode="widthFix"></image>
        <!-- <view class='sign-level'>
            <image class="img-map map-level" src="{{signLevelMap}}" mode="widthFix"></image>
        </view> -->
        <view class='sign-handle {{loadclass}}'>
            <button class='hidebtn sign-btn btnclick-anim anim-sign' bindtap="submitSignIn">
                <image src="../../asset/image/sign-btn.png" mode="widthFix"></image>
            </button>
        </view>
    </view>
</view>